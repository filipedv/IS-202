@using OBLIG1.Models
@model IEnumerable<OBLIG1.Models.Registerforer>



@{
    ViewData["Title"] = "Registerfører - Dashboard";
}

<div class="container mt-5">
    <h1 class="text-3xl font-bold mb-3 text-success">Velkommen, Registerfører!</h1>
    <p class="lead">Her kan du registrere og se oversikt over hindringer.</p>

    <div class="mt-4 d-flex gap-3">
        <a asp-action="DataForm" asp-controller="Obstacle" class="btn btn-primary">Registrer hinder</a>
        <a asp-action="Overview" asp-controller="Obstacle" class="btn btn-secondary">Se oversikt</a>
    </div>
</div>